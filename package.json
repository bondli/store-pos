{
  "name": "store-pos",
  "description": "An easy-to-use store point of sale system",
  "version": "0.0.1",
  "license": "MIT",
  "main": "dist/electron/main.js",
  "scripts": {
    "before:pack": "tsc -p tsconfig.electron.json",
    "build": "vite build",
    "dev": "npm run before:pack && vite --open=false -m electron",
    "pack:win": "npm run build && npm run before:pack && electron-builder --win --x64",
    "pack:mac": "npm run build && npm run before:pack && electron-builder --mac",
    "postinstall": "electron-builder install-app-deps && npm run rebuild-sqlite3",
    "lint": "npm run prettier && npm run eslint",
    "eslint": "eslint ./ --fix --ext .ts,.tsx,.js,.jsx",
    "prettier": "prettier --write ./**/*.{ts,tsx,js,jsx,html,css,scss,less}",
    "eslint:init": "eslint --init",
    "update-hooks": "npm run update-x && npm run update-x-dev",
    "update-x": "npm i js-xhttp js-xxx js-xcall",
    "update-x-dev": "npm i js-xcmd -D",
    "clean": "xcmd clean",
    "time": "xcmd update-time",
    "rebuild-sqlite3": "electron-rebuild -f -w sqlite3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run time && npm run update-hooks"
    }
  },
  "dependencies": {
    "@ant-design/icons": "^5.3.6",
    "@ant-design/plots": "^1.1.0",
    "antd": "^5.16.4",
    "axios": "^1.6.8",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.11",
    "decimal.js": "^10.5.0",
    "electron-log": "^5.1.2",
    "electron-store": "^8.2.0",
    "exceljs": "^4.4.0",
    "express": "^4.19.2",
    "form-render": "latest",
    "lodash-es": "^4.17.21",
    "multer": "^1.4.5-lts.2",
    "react": "^18.2.0",
    "react-countup": "6.5.3",
    "react-dom": "^18.2.0",
    "react-quill": "^2.0.0",
    "sequelize": "^6.37.3",
    "sql.js": "^1.8.0",
    "sqlite3": "5.1.6",
    "table-render": "latest",
    "timeago.js": "^4.0.2",
    "uuid": "^10.0.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@types/body-parser": "^1.19.5",
    "@types/express": "^4.17.21",
    "@types/multer": "^1.4.12",
    "@types/node": "^20.12.12",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "@vitejs/plugin-react": "^2.0.1",
    "electron": "^30.0.1",
    "electron-builder": "^24.13.3",
    "electron-packager": "^17.1.2",
    "electron-rebuild": "^3.2.9",
    "eslint": "^9.1.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-spellcheck": "0.0.19",
    "eslint-plugin-zob": "^0.4.0",
    "husky": "^7.0.4",
    "js-xcmd": "^1.3.0",
    "less": "^4.2.0",
    "prettier": "^2.6.2",
    "terser": "^5.14.2",
    "vite": "^3.0.8",
    "vite-plugin-electron": "^0.9.0",
    "vite-plugin-eslint": "^1.8.1"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "author": "bondli",
  "homepage": "https://github.com/bondli/store-pos",
  "repository": {
    "type": "git",
    "url": "https://github.com/bondli/store-pos.git"
  },
  "keywords": [
    "pos",
    "store",
    "retail",
    "electron",
    "react"
  ],
  "build": {
    "productName": "StorePos",
    "appId": "com.storepos.react.electron",
    "asar": true,
    "copyright": "Copyright Â© 2025 ${author}",
    "files": [
      "dist/**/*"
    ],
    "directories": {
      "output": "release/${version}"
    },
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ],
      "icon": "dist/electron/icons/icon.ico"
    },
    "mac": {
      "artifactName": "${productName}-Mac-${arch}-${version}.${ext}",
      "target": {
        "target": "dmg",
        "arch": [
          "arm64",
          "x64"
        ]
      },
      "icon": "dist/electron/icons/Icon.icns",
      "entitlements": "dist/electron/entitlements.mac.plist",
      "entitlementsInherit": "dist/electron/entitlements.mac.plist",
      "hardenedRuntime": true
    },
    "nsis": {
      "language": 2052,
      "artifactName": "${productName}-Windows-${version}-Setup.${ext}",
      "allowElevation": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "deleteAppDataOnUninstall": true,
      "allowToChangeInstallationDirectory": true,
      "oneClick": false,
      "installerIcon": "dist/electron/icons/icon.ico",
      "uninstallerIcon": "dist/electron/icons/icon.ico",
      "installerHeaderIcon": "dist/electron/icons/icon.ico"
    },
    "extraResources": [
      {
        "from": "node_modules/sql.js/dist/sql-wasm.wasm",
        "to": "sql.js/sql-wasm.wasm"
      }
    ]
  },
  "time": "3020291721072022"
}
